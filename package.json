{
  "name": "art-house-holding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sitemap": "next-sitemap --next-sitemap.config.js",
    "build:local": "APP_ENV=local next build",
    "build:dev": "APP_ENV=dev next build",
    "build:prod": "APP_ENV=prod next build",
    "deploy:local": "yarn build:local && pm2 start pm2.config.js --env local",
    "deploy:dev": "yarn build:dev && pm2 start pm2.config.js --env dev",
    "deploy:prod": "yarn build:prod && pm2 start pm2.config.js --env prod"
  },
  "dependencies": {
    "@next/font": "^14.0.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@sanity/dashboard": "^3.1.6",
    "@sanity/document-internationalization": "^2.0.2",
    "@sanity/vision": "^3.23.4",
    "classnames": "^2.5.1",
    "i18next": "^23.7.16",
    "next": "14.0.4",
    "next-sanity": "^7.0.7",
    "next-seo": "^6.4.0",
    "ni18n": "^1.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.0.0",
    "react-redux": "^9.0.4",
    "react-scroll": "^1.9.0",
    "sanity": "^3.23.4",
    "sass": "^1.69.7",
    "swiper": "^11.0.5",
    "next-sitemap": "^4.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-scroll": "^1.8.10",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "typescript": "^5"
  }
}